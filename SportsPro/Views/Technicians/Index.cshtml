@model List<Technician>
@{
    ViewBag.Title = "Technician Manager";
}

<h1 class="jumbotron">Technician Manager</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Phone</th>
            <th scope="col"></th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var t in @Model)
        {
            <tr>
                <td>@t.Name</td>
                <td>@t.Email</td>
                <td>@t.Phone</td>
                <td>
                    <a asp-controller="Technicians" asp-action="Edit" asp-route-id=@t.TechnicianID class="btn btn-primary">Edit</a>
                </td>
                <td>
                    <a asp-controller="Technicians" asp-action="Delete" asp-route-id=@t.TechnicianID class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<br />
<br />
<br />
<a asp-controller="Technicians" asp-action="Add" class="btn btn-primary">Add Technician</a>